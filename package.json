{
  "name": "my-spendings",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:server": "nodemon -r dotenv/config server.js",
    "start:client": "cd client && npm run start",
    "start:all": "concurrently --kill-others \"npm run start:server\" \"npm run start:client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json,md,sass}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": "10.16.0",
    "npm": "6.10.0"
  }
}
